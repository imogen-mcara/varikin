@book{jones_grammar_2011,
    author = {Jones, Barbara},
    abstract = {This book is a description of an Australian language from the Great Sandy Desert of north Western Australia. It is a description of a language that has a detailed case system, complex cross-referencing by bound pronouns and a word order that is determined by pragmatics rather than syntax. The description benefits from the lively natural language examples used by the principal language consultant. By comparisons with other languages of the Western Desert the study highlights some of the features that group the northern Western Desert languages and distinguish them from those in the south. It also draws some comparisons with the northern neighbours of the Western Desert belonging to the Marrngu and Ngumpin groups.},
    address = {{Canberra}},
    file = {/Users/u1123114/Zotero/storage/48HQ5P8V/20170206_164855.jpg;/Users/u1123114/Zotero/storage/QAB9MIG4/Jones_2011_A Grammar of Wangkajunga.pdf;/Users/u1123114/Zotero/storage/W2RZ9BN6/20170206_164905.jpg;/Users/u1123114/Zotero/storage/WE82ARUS/20170206_164930.jpg},
