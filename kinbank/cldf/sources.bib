@article{borges_aspectos_2006,
    author = {Borges, Monica Veloso},
    abstract = {Resumo: A l\'ingua Av\'a-Canoeiro, pertencente ao Subgrupo IV das l\'inguas Tupi-Guarani , \'e falada por vinte e duas pessoas. A maioria desses ind\'igenas vive na aldeia Canoan\~a, na Ilha do Bananal, a cinq\"uenta e quatro quil\^ometros da cidade de Formoso do Araguaia, no Estado do Tocantins. H\'a tamb\'em uma fam\'ilia que mora na aldeia Java\'e de Boto Velho, pr\'oxima ao munic\'ipio de Lagoa da Confus\~ao, tamb\'em na Ilha. Seis Av\'a-Canoeiro vivem na Terra Ind\'igena pr\'oxima \`a cidade de Mina\c{c}u, em Goi\'as, localizada a oitenta quil\^ometros daquele munic\'ipio. Esta tese, cuja fundamenta\c{c}\~ao te\'orica \'e a Tipologia Funcional, aborda aspectos fonol\'ogicos e morfossint\'aticos dessa l\'ingua brasileira pouqu\'issimo conhecida, bem como fornece ao leitor um panorama sobre a situa\c{c}\~ao socioling\"u\'istica desse povo em nossos d as. S\~ao temas discutidos na parte relativa \`a fonologia: 1) os fonemas e seus alofones; 2) aspectos fonol\'ogicos diacr\^onicos; 3) processos fonol\'ogicos e morfofonol\'ogicos; 4) a constitui\c{c}\~ao sil\'abica; e 5) o padr\~ao acentual. No que diz respeito \`a morfossintaxe, discuto: 1) as classes de palavras abertas (nome, verbo e adv\'erbio) e fechadas (pronomes pessoais, demonstrativos, part\'iculas e posposi\c{c}\~oes); 2) as ora\c{c}\~oes simples e coordenadas; 3) os tipos oracionais; 4) a ordem frasal; e 5) a hip\'otese de a l\'ingua ser de estrutura ativa.},
    file = {/Users/u1123114/Zotero/storage/M8PJEAPX/Borges - 2006 - Aspectos fonologicos e morfossintaticos da lingua .pdf;/Users/u1123114/Zotero/storage/H3Z2VPWP/270406.html},
    keywords = {avac1239,IC,kinbank,TP7},
    langid = {portuguese},
