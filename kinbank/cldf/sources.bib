    file = {/Users/u1123114/Zotero/storage/H899MRM4/Silva - 2009 - Cativando Maira a sobrevivência avá-canoeiro no .pdf;/Users/u1123114/Zotero/storage/AA8HL4MP/1568.html},
    keywords = {avac1239,IC,kinbank,TP6},
    langid = {portuguese},
