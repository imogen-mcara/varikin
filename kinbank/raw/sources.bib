@article{silva_cativando_2009,
  title = {{Cativando Maira : a sobreviv\^encia av\'a-canoeiro no alto rio Tocantins}},
  shorttitle = {{Cativando Maira}},
  author = {da Silva, Cristhian Te{\'o}filo},
  year = {2009},
  month = aug,
  abstract = {Esta etnografia busca compreender as representa\c{c}\~oes e pr\'aticas dos av\'a-canoeiros sobre um contexto de domina\c{c}\~ao espec\'ifico.  A situa\c{c}\~ao hist\'ori-ca dos av\'a-canoeiros ser\'a abordada como uma situa\c{c}\~ao extrema decorrente da implementa\c{c}\~ao de um regime tutelar exercido para assegurar a sobreviv\^en-cia dos av\'a-canoeiros enquanto uma etnia ``em vias de extin\c{c}\~ao''.  O presente trabalho inscreve-se assim em uma tradi\c{c}\~ao de pesquisas etnogr\'aficas de ca-sos particulares sobre o \'indio sob tutela.  Seu objeto pode ser definido como os mecanismos de transforma\c{c}\~ao dos av\'a-canoeiros em ``\'indios'' ou objetos do poder tutelar, bem como a aco-moda\c{c}\~ao dos av\'a-canoeiros a estes mecanismos e estes a eles.  A tese ser\'a dividida como segue: 1) revis\~ao cr\'itica da literatura antropo-l\'ogica recente sobre os av\'a-canoeiros, 2) discuss\~ao te\'orica sobre a import\^ancia das rela\c{c}\~oes de poder para a compreens\~ao da vida social dos av\'a-canoeiros sob tutela, 3) hist\'orico do contato, 4) etnografia da cena tutelar, e 5) conclus\~ao mediante considera\c{c}\~oes cr\'iticas sobre as no\c{c}\~oes paternalistas de tutela para os av\'a-canoeiros e a responsabilidade dos antrop\'ologos diante das pol\'iticas indigenistas e das demandas pol\'iticas dos \'indios.  \_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_\_ ABSTRACT},
  copyright = {Acesso Aberto},
  langid = {portuguese},
  keywords = {avac1239,IC,kinbank,TP6},
  file = {/Users/u1123114/Zotero/storage/H899MRM4/Silva - 2009 - Cativando Maira  a sobrevivência avá-canoeiro no .pdf;/Users/u1123114/Zotero/storage/AA8HL4MP/1568.html}
}

@article{sinha_kinship_2012,
  title = {Kinship {{Terms}} in {{Nepali Language}} and Its {{Morphology}}},
  author = {Sinha, Prajadhip and Sarma, Bairab and Shyam Purkayastha, Bipul},
  year = {2012},
  month = nov,
  journal = {International Journal of Computer Applications},
  volume = {58},
  number = {9},
  pages = {43--49},
  issn = {09758887},
  doi = {10.5120/9314-3546},
  abstract = {Kinship relations are blood relations or those relations which are created naturally. Kinship is a method of acknowledging relations and it is a social bond initiated by blood or genetic as well as marriage. Kinship is important in all cultures and in all human interaction. Kinship is important to anthropology because anthropology is the study of human behaviour and human behaviour is variable. According to M.Lamp[12] to understand fully the nature of kinship systems it is necessary to understand what kind of linguistic elements these are, and what kind of linguistic relationships. The kinship terminology of any language is a natural meeting point for the disciplines of anthropology and linguistics [6]. Like other languages of the world, Nepali kinship terms are also common for Nepali Language. Nepali1 or Nepalese (नपे ाली) is a language in the Indo-Aryan branch of the Indo-European language family. It takes its root from Sanskrit, the classical language of India. This paper explores the area of kinship terms in Nepali language, and its outlines the standard kinship relations, associated set of terms in the language. The formations of such terms are also elaborated with grammatical analysis. Kinship terms form a considerable part of the WordNet in any language because the kinship terms interacts each other with different relational characteristics of WordNet. Nepali is a morphologically rich language and Kinship terms form an important aspect in morphology of Nepali Language. There are many number of morphological studies in Nepali but most of them are descriptive in nature. Morphology of Nepali has not yet been fully analyzed from computational perspective.},
  langid = {english},
  keywords = {east1436,IE45,kinbank,SP},
  file = {/Users/u1123114/Zotero/storage/QS59S8DA/Sinha et al. - 2012 - Kinship Terms in Nepali Language and its Morpholog.pdf}
}

